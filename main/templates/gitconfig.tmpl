[user]
    name =
    email =
[core]
    editor = vim
    mergeoptions = --no-edit
[push]
    default = simple
[color]
    ui = true
    status = auto
[advice]
    pushNonFastForward = false
[merge]
    stat = true
    tool = vimdiff
[pull]
    rebase = true
[rebase]
    autostash = true
[color "branch"]
    local = yellow
    remote = green
    current = yellow reverse
[diff]
    tool = vimdiff
[difftool]
    prompt = false
[alias]
    co = checkout
    cob = checkout -b
    br = branch
    brD = branch -D
    brv = branch -v
    brr = branch -r
    com = checkout master
    cod = checkout development
    st = status
    aa = add -A .
    df = diff
    pom = push origin master
    pod = push origin development
    graph = log --all --decorate --oneline --graph
    cm = commit -m
    po = push origin
    poh = push origin HEAD
    unstage = reset --soft HEAD^
    amend = commit --amend -m
    cp = cherry-pick
    pf = pull --ff
